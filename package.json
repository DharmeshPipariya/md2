{
  "name": "md2",
  "version": "0.0.1",
  "description": "Angular2 based Material Design components, directives and services are Accordion, Autocomplete, Collapse, Colorpicker, Datepicker, Dialog(Modal), Menu, Multiselect, Select, Switch, Tabs, Tags(Chips), Toast and Tooltip.",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run watch\" \"npm run lite\" ",
    "lite": "lite-server",
    "watch": "gulp watch",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "main": "gulpfile.js",
  "keywords": [
    "angular2",
    "angularjs",
    "material",
    "component",
    "accordion",
    "autocomplete",
    "collapse",
    "colorpicker",
    "datepicker",
    "dialog",
    "modal",
    "menu",
    "dropdown",
    "multiselect",
    "select",
    "switch",
    "tabs",
    "tags",
    "chips",
    "toast",
    "tooltip"
  ],
  "author": "Dharmesh Pipariya <pipariyadharmesh@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dharmeshpipariya/md2.git"
  },
  "bugs": {
    "url": "https://github.com/dharmeshpipariya/md2/issues"
  },
  "homepage": "https://github.com/dharmeshpipariya/md2#readme",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.1",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.4",
    "systemjs": "0.19.27",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12",
    "angular2-in-memory-web-api": "0.0.14",
    "bootstrap": "^3.3.6"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "del": "^2.2.1",
    "gulp": "^3.9.1",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}

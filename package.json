{
  "name": "md2",
  "version": "0.0.1",
  "description": "Angular2 based Material Design components, directives and services are Accordion, Autocomplete, Collapse, Colorpicker, Datepicker, Dialog(Modal), Menu, Multiselect, Select, Switch, Tabs, Tags(Chips), Toast and Tooltip.",
  "scripts": {
    "clean": "gulp clean",
    "compile": "gulp compile",
    "build": "gulp build",
    "start": "concurrent --kill-others \"gulp watch\" \"npm run lite\"",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "main": "gulpfile.js",
  "keywords": [
    "angular2",
    "angularjs",
    "material",
    "component",
    "accordion",
    "autocomplete",
    "collapse",
    "colorpicker",
    "datepicker",
    "dialog",
    "modal",
    "menu",
    "dropdown",
    "multiselect",
    "select",
    "switch",
    "tabs",
    "tags",
    "chips",
    "toast",
    "tooltip"
  ],
  "author": "Dharmesh Pipariya <pipariyadharmesh@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dharmeshpipariya/md2.git"
  },
  "bugs": {
    "url": "https://github.com/dharmeshpipariya/md2/issues"
  },
  "homepage": "https://github.com/dharmeshpipariya/md2#readme",
  "dependencies": {
    "moment": "2.13.0"
  },
  "devDependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "concurrently": "^2.0.0",
    "core-js": "^2.4.0",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^4.3.3",
    "gulp-typescript": "^2.12.0",
    "gulp-uglify": "^2.0.0",
    "lite-server": "^2.2.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "systemjs-builder": "0.15.23",
    "tslint": "^3.5.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "zone.js": "^0.6.12"
  }
}
